{
  "scripts": {
    "tsc": "tsc",
    "expo:start": "expo start -c",
    "expo:start:ios": "expo start --ios",
    "expo:start:android": "expo start --android",
    "build": "yarn run tsc --",
    "watch": "yarn run build -- -w",
    "start": "concurrently \"yarn watch\" \"yarn expo:start\"",
    "start:ios": "concurrently \"yarn watch\" \"yarn expo:start:ios\"",
    "start:android": "concurrently \"yarn watch\" \"yarn expo:start:android\"",
    "eject": "expo eject",
    "test": "node_modules/.bin/jest test/**/*.spec.tsx",
    "test-commit": "jest",
    "lint": "tslint 'src/**/*.{ts,tsx}' --fix -c ./tslint.json",
    "pack:android": "yarn build && expo build:android",
    "pack:android:staging": "yarn build && expo build:android --release-channel staging",
    "publish:staging": "yarn build && expo publish --release-channel staging",
    "pack:ios": "yarn build && expo build:ios --release-channel staging",
    "clearCaches": "watchman watch-del-all && rm -rf $TMPDIR/haste-map-metro-* && rm -rf $TMPDIR/metro*",
    "prepare": "patch-package"
  },
  "packagerOpts": {
    "assetExts": [
      "ttf",
      "otf"
    ]
  },
